def bubble_sort(arr):
    n = len(arr)
    for i in range(n - 1):
        for j in range(n - i - 1):
            if arr[j] > arr[j + 1]:
                arr[j], arr[j + 1] = arr[j + 1], arr[j]
    return arr

a = [64, 34, 25, 12, 22, 11, 90]
print("Original array:", a)

sorted_arr = bubble_sort(a)
print("Sorted array:", sorted_arr)
