def binary_search(arr, target):
    low = 0
    high = len(arr) - 1

    while low <= high:
        mid = (low + high) // 2 
        mid_value = arr[mid]

        if mid_value == target:
            return mid 
        elif mid_value < target:
            low = mid + 1 
        else:
            high = mid - 1 

    return -1 
    
a = [2, 5, 8, 12, 16, 23, 38, 56, 72, 91]

# Case 1: element found
target = 5
result = binary_search(a, target)
if result != -1:
    print("Element found at index", result)
else:
    print("Element not found")

# Case 2: element not found
search_target_not_found = 100
result_not_found = binary_search(a, search_target_not_found)
if result_not_found != -1:
    print("Element found at index", result_not_found)
else:
    print("Element not found")
