def linear_search(arr, target):
    for i in range(len(arr)):
        if arr[i] == target:
            return i  
    return -1  


a = [2, 5, 8, 12, 16, 23, 38, 56, 72, 91]

# Case 1: element found
target = 5
result = linear_search(a, target)
if result != -1:
    print("Element found at index", result)
else:
    print("Element not found")

# Case 2: element not found
search_target_not_found = 100
result_not_found = linear_search(a, search_target_not_found)
if result_not_found != -1:
    print("Element found at index", result_not_found)
else:
    print("Element not found")
